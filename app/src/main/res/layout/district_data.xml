<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/consLayMain"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/consLayData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintRight_toRightOf="@id/consLayMain"
        app:layout_constraintLeft_toLeftOf="@id/consLayMain"
        app:layout_constraintTop_toTopOf="@id/consLayMain">
        <TextView
            android:id="@+id/textViewDistName"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="5dp"
            android:layout_marginStart="5dp"
            android:textSize="15dp"
            android:textStyle="bold"
            android:background="#04198E"
            android:textColor="@color/white"
            android:text="Name"
            app:layout_constraintLeft_toLeftOf="@id/consLayData"
            app:layout_constraintTop_toTopOf="@id/consLayData"/>

        <TextView
            android:id="@+id/textViewDeltaConfirmed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#D50000"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:text="delta"
            android:gravity="right"
            app:layout_constraintBottom_toTopOf="@+id/textViewConfirmedData"
            app:layout_constraintRight_toRightOf="@+id/textViewConfirmedData" />

        <TextView
            android:id="@+id/textViewConfirmedData"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginStart="5dp"
            android:background="#D50000"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:text="Name"
            android:gravity="right"
            app:layout_constraintLeft_toRightOf="@id/textViewDistName"
            app:layout_constraintTop_toTopOf="@id/textViewDistName"
            app:layout_constraintBottom_toBottomOf="@id/textViewDistName"
            app:layout_constraintRight_toLeftOf="@+id/textViewActiveData" />

        <TextView
            android:id="@+id/textViewActiveData"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:layout_marginStart="5dp"
            android:background="#304FFE"
            android:textSize="15dp"
            android:text="Name"
            android:gravity="right"
            app:layout_constraintBottom_toBottomOf="@+id/textViewConfirmedData"
            app:layout_constraintTop_toTopOf="@+id/textViewConfirmedData"
            app:layout_constraintLeft_toRightOf="@+id/textViewConfirmedData"
            app:layout_constraintRight_toLeftOf="@+id/textViewRecoveredData"/>

        <TextView
            android:id="@+id/textViewDeltaRecovered"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#058C3D"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:text="delta"
            android:gravity="right"
            app:layout_constraintBottom_toTopOf="@+id/textViewRecoveredData"
            app:layout_constraintRight_toRightOf="@+id/textViewRecoveredData" />

        <TextView
            android:id="@+id/textViewRecoveredData"
            android:layout_width="67dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginStart="5dp"
            android:background="#058C3D"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:text="Name"
            android:gravity="right"
            app:layout_constraintLeft_toRightOf="@+id/textViewActiveData"
            app:layout_constraintTop_toTopOf="@+id/textViewActiveData"
            app:layout_constraintBottom_toBottomOf="@+id/textViewActiveData"
            app:layout_constraintRight_toLeftOf="@+id/textViewDeceasedData"/>

        <TextView
            android:id="@+id/textViewDeltaDeceased"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#636363"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:text="delta"
            android:gravity="right"
            app:layout_constraintBottom_toTopOf="@+id/textViewDeceasedData"
            app:layout_constraintRight_toRightOf="@+id/textViewDeceasedData" />

        <TextView
            android:id="@+id/textViewDeceasedData"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginStart="5dp"
            android:layout_marginRight="5dp"
            android:background="#636363"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:text="Name"
            android:gravity="right"
            app:layout_constraintTop_toTopOf="@+id/textViewRecoveredData"
            app:layout_constraintBottom_toBottomOf="@+id/textViewRecoveredData"
            app:layout_constraintRight_toRightOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>